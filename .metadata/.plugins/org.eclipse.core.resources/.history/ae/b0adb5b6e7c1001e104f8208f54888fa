package PageObjects;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class LoanCalculator extends BasePage {

	public LoanCalculator(WebDriver driver) {
		super(driver);
		// TODO Auto-generated constructor stub
	}

	
	@FindBy(xpath="//li[@id='emi-calc']")
	WebElement EMI_Calculator;
	
	@FindBy(xpath="//li[@id='loan-amount-calc']")
	WebElement LoanAount_Calculator;
	
	@FindBy(xpath="//li[@id='loan-tenure-calc']")
	WebElement Tennure_Calculator;
	
	@FindBy(xpath="//li[@id='interest-rate-calc']")
	WebElement Interest_Rate_Calculator;
	
	
	//ACTION METHODS
	
	
	
	
}
