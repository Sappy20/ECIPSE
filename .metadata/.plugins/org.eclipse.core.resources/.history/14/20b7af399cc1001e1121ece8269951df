	}
	
	@Test(priority=24)
	public void emiCalculatorIs_Present_Validation()
	{
	LOAN_Calculator_UI  Calc_UI=new LOAN_Calculator_UI(driver);
	Calc_UI.Validation_Of_EMICalculator();
	}
	
	@Test(priority=24)
	public void emiCalculatorIs_Present_Validation()
	{
	LOAN_Calculator_UI  Calc_UI=new LOAN_Calculator_UI(driver);
	Calc_UI.Validation_Of_EMICalculator();
